package com.office.school;

public class MainClass {

	public static void main(String[] args) {
		Student student = new Student();
		Student student2 = new Student("박찬호",1, 17, "computer", 3.5f);
		Student student3 = new Student("홍길동",2,16,"sports", 2.0f);
		Student student4 = new Student("임환묵", 3,61,"정치외교",4.5f);
		
		student.printInfo();
		student2.printInfo();
		student3.printInfo();
		student4.printInfo();

	}

}
