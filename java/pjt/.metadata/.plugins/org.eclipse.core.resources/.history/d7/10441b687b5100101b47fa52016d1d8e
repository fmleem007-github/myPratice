package com.office.ourservice;

public class MainClass {

	public static void main(String[] args) {
		int num1 = 10;
		Car myCar = new Car();
		Car brotherCar = new Car();

		System.out.println(myCar);
		System.out.println(brotherCar);
		System.out.println(myCar.carLength);
		brotherCar.carColor = "yellow";
		System.out.println(brotherCar.carColor);
		
		myCar.GoForeward();
		brotherCar.GoBackward();
	}

}
